# ============================================
# DATABASE CONFIGURATION
# ============================================
# PostgreSQL connection settings
DB_HOST=localhost
DB_PORT=5432
DB_NAME=LabSyncDb
DB_USER=postgres
DB_PASSWORD=your_postgres_password_here

# ============================================
# JWT AUTHENTICATION
# ============================================
# Secret key for JWT tokens (minimum 32 characters)
JWT_KEY=minimum_32_characters_long_secret_key_here
JWT_ISSUER=LabSyncServer
JWT_AUDIENCE=LabSyncAgent

# ============================================
# SERVER CONFIGURATION
# ============================================
# ASP.NET Core environment: Development, Staging, Production
ASPNETCORE_ENVIRONMENT=Development
# Server URLs (comma separated)
ASPNETCORE_URLS=http://localhost:5000;https://localhost:5001

# ============================================
# CORS SETTINGS
# ============================================
# Allowed origins for frontend (comma separated)
CORS_ALLOWED_ORIGINS=http://localhost:3000

# ============================================
# SIGNALR CONFIGURATION
# ============================================
# Path for SignalR hub
SIGNALR_HUB_PATH=/agentHub

# ============================================
# TEST DATA GENERATION
# ============================================
# Generate fake data for testing (true/false)
GENERATE_TEST_DATA=true
# Number of test devices to generate
TEST_DEVICES_COUNT=10